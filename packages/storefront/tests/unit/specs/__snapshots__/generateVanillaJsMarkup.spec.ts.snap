exports[`generateVanillaJsMarkup() > should generate correct Vanilla JS markup for button 1`] = `
"<!doctype html>
<html lang="en">
<head>
  <title></title>
</head>
<body>

<p-button aria="{'aria-haspopup':true,'aria-label':'Some more descriptive label'}" type="button" compact="true" icon="add">
  Open Banner
</p-button>

<p-banner dismiss-button="false" heading="Some heading" heading-tag="h4" state="warning" />

<script>
  const button = document.querySelector("p-button");
  const banner = document.querySelector("p-banner");
  button.addEventListener('click', () => (banner.open = true));
  banner.addEventListener('dismiss', () => (banner.open = false));
</script>
</body>
</html>"
`;

